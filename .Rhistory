clientes_fieles <- data_final %>%
group_by(id_cliente_enc) %>%
summarise(n_compras = n_distinct(num_ticket)) %>%
filter(n_compras >= 5) %>%
pull(id_cliente_enc)
data_filtrada_clientes_fieles <- data_final %>%
filter(id_cliente_enc %in% clientes_fieles)
